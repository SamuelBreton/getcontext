{
  "name": "context-api",
  "version": "1.0.0",
  "description": "API for providing contextual data.",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --inspect --verbose src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
